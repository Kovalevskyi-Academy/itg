{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CHOICE",
  "type": "object",
  "properties": {
    "choiceText": {
      "description": "Description of the essence of the choice.",
      "type": "string",
      "minLength": 1,
      "example": "Current Pick: Take a Cockroach Assfucking Journey!"
    },
    "nextNodeId": {
      "description": "Unique identifier of next Node.",
      "type": "string",
      "minLength": 1,
      "example": "node01"
    }
  },
  "required": ["nextNodeId", "choiceText"]
}